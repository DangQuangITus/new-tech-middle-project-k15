{{!-- <h1>{{title}}</h1> --}}
{{!-- <p>Welcome to {{title}}</p> --}}
{{!-- <h3>Xây Dựng Hệ Thống Đặt xe máy qua thông tin khách hàng nhập vào</h3> --}}

<div class="row">
    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1"></div>
    <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">Nhập thông tin vị trí: </h3>
            </div>
            <div class="panel-body">
                <form id="formInfo">
                    <div class="">
                        <div class="form-group ">
                            <label for="username">Ho Ten: &#160;</label>
                            <input onkeypress="onKeypress()" type="text" class="form-control" id="username" placeholder="Ho Ten">
                        </div>&#160;&#160;
                        <div class="form-group">
                            <label for="phonenumber">So Dien Thoai: &#160;</label>
                            <input onkeypress="onKeypress()" type="text" class="form-control" id="phonenumber"
                                placeholder="So dien thoai">
                        </div>
                    </div>
                    <br />
                    <div class="form-group ">
                        <label for="address">Dia chi don khach: &#160;</label>
                        <input onkeypress="onKeypress()" onchange="onChange()" type="text" class="form-control" id="address"
                            placeholder="Dia chi">
                        {{!-- <p class="help-block">Example block-level help text here.</p> --}}
                    </div>
                    <div class="form-group">
                        <label for="note">Ghi chu: &#160;</label>
                        <textarea onkeypress="onKeypress()" class="form-control" cols="4" rows="2" id="note"
                            placeholder="ghi chu."></textarea>
                    </div>
                    <button type="submit" class="btn btn-default">Submit</button>
                </form>
            </div>
        </div>
    </div> &#160;
    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
        <div id="google-map-api" style="width: 600px; height: 500px; background: grey" class="text-center">
        </div>
        <div id="panel" style="width: 600px; "></div>

        <button id="getaddressdemo" type="button" class="btn btn-danger">Get Address demo</button>

    </div>
</div>

<div class="row">

    <table class="table table-hover table-bordered">
        <thead>
            <tr>
                <th>id</th>
                <th>name</th>
                <th>address</th>
                <th>phone</th>
                <th>notes</th>
                <th>status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
            {{#each data}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.name}}</td>
                <td>{{this.address}}</td>
                <td>{{this.phone}}</td>
                <td>{{this.notes}}</td>
                <td>{{this.status}}</td>
                <td>
                    <button id="getmapid" value={{this.id}} type="button" class="btn btn-default">get map</button>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>

</div>

<script type="text/javascript">

    document.getElementById("getmapid").addEventListener('click', function () {
        console.log($(this).val());
    })
</script>