{{!-- <p>Welcome to {{title}}</p> --}}
{{!-- <h3>Xây Dựng Hệ Thống Đặt xe máy qua thông tin khách hàng nhập vào</h3> --}}

<div class="row">
    <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2"></div>
    <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8">


        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">Nhập thông tin vị trí: </h3>
            </div>
            <div class="panel-body">
                <form onsubmit="onSubmit()">
                    <div class=" form-inline">
                        <div class="form-group ">
                            <label for="username">Ho Ten: &#160;</label>
                            <input onkeypress="onKeypress()" type="text" class="form-control" id="username" placeholder="Ho Ten">
                        </div>&#160;&#160;
                        <div class="form-group">
                            <label for="phonenumber">So Dien Thoai: &#160;</label>
                            <input onkeypress="onKeypress()" type="text" class="form-control" id="phonenumber"
                                placeholder="So dien thoai">
                        </div>
                    </div>
                    <br />
                    <div class="form-group ">
                        <label for="address">Dia chi don khach: &#160;</label>
                        <input onkeypress="onKeypress()" onchange="onChange()" type="text" class="form-control" id="address"
                            placeholder="Dia chi">
                        {{!-- <p class="help-block">Example block-level help text here.</p> --}}
                    </div>
                    <div class="form-group">
                        <label for="note">Ghi chu: &#160;</label>
                        <textarea onkeypress="onKeypress()" class="form-control" cols="4" rows="3" id="note"
                            placeholder="ghi chu."></textarea>
                    </div>
                    <button type="submit" class="btn btn-default">Submit</button>
                </form>
            </div>
        </div>

        <div id="googlemapapi">

        </div>

    </div>
    <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2"></div>
</div>

<script type="text/javascript">
    onSubmit = () => {
        var info = {
            name: $('#username').val(),
            sdt: $('#phonenumber').val(),
            address: $('#address').val(),
            note: $('#note').val()
        }
        console.log(info);
    }
    onKeypress = () => {
        var info = {
            name: $('#username').val(),
            sdt: $('#phonenumber').val(),
            address: $('#address').val(),
            note: $('#note').val()
        }
        console.log(info);
    }
    onChange = () => {
        var info = {
            name: $('#username').val(),
            sdt: $('#phonenumber').val(),
            address: $('#address').val(),
            note: $('#note').val()
        }
        console.log(info);
    }
</script>